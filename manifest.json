{
  "manifest_version": 3,
  "name": "Notion Favicon Fixer",
  "version": "1.0",
  "icons": {
    "128": "icon.jpeg"
  },
  "content_scripts": [
    {
      "matches": ["https://notion.so/*", "https://www.notion.so/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage"]
}
